{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Electron CommonJS",
  "extends": "./base.json",
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
  }
}
